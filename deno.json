{
	"tasks": {
		"dev": "deno run --watch --allow-net -R=static,branches,logs -W=branches,logs main.ts",
		"main": "deno run --allow-net -R=static,branches,logs -W=branches,logs main.ts",
		"test": "deno test --allow-net -R=static,branches,logs -W=branches,logs",
		"ptest": "deno test --allow-net -R=static,branches,logs -W=branches,logs --parallel",
		"compile-win": "deno compile --target x86_64-pc-windows-msvc --allow-net -R=static,branches,logs -W=branches,logs main.ts",
		"compile-linux": "deno compile --target x86_64-unknown-linux-gnu --allow-net -R=static,branches,logs -W=branches,logs main.ts",
		"compile": "deno compile --allow-net -R=static,branches,logs -W=branches,logs main.ts",
		"ctest": "rm -r coverage || deno test --allow-net -R=static,branches,logs -W=branches,logs --parallel --coverage && python -m http.server -d coverage/html 8080 2> /dev/null"
	},
	"imports": {
		"@oak/oak": "jsr:@oak/oak@^17.1.6",
		"@std/assert": "jsr:@std/assert@^1.0.14",
		"@std/fs": "jsr:@std/fs@^1.0.19",
		"@std/path": "jsr:@std/path@^1.1.2",
		"@std/testing": "jsr:@std/testing@^1.0.15",
		"@zip-js/zip-js": "jsr:@zip-js/zip-js@^2.7.73"
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 120,
		"semiColons": true,
		"exclude": [
			"coverage",
			"branches",
			"logs",
			"static"
		],
		"useBraces": "preferNone"
	},
	"version": "0.5.0"
}
